{
    "version": "2.1.0",
    "runs": [
      {
        "tool": {
          "driver": {
            "name": "CodeQL",
            "version": "2.20.4",
            "rules": [
              {
                "id": "actions/missing-workflow-permissions",
                "shortDescription": {
                  "text": "Workflow does not contain permissions"
                },
                "fullDescription": {
                  "text": "Workflows should contain permissions to provide a clear understanding of who has permissions to run the workflow."
                },
                "helpUri": "https://docs.github.com/en/actions/writing-workflows/choosing-what-your-workflow-does/assigning-permissions-to-jobs",
                "properties": {
                  "tags": ["actions", "external/cwe/cwe-275", "maintainability"],
                  "security-severity": "medium"
                }
              }
            ]
          }
        },
        "results": [
          {
            "ruleId": "actions/missing-workflow-permissions",
            "level": "note",
            "message": {
              "text": "Actions Job or Workflow does not set permissions"
            },
            "locations": [
              {
                "physicalLocation": {
                  "artifactLocation": {
                    "uri": ".github/workflows/alerts.yml",
                    "index": 0
                  },
                  "region": {
                    "startLine": 11,
                    "startColumn": 5,
                    "endLine": 44,
                    "endColumn": 65
                  }
                }
              }
            ]
          }
        ]
      }
    ]
  }